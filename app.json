{
  "expo": {
    "name": "",
    "slug": "quick-shop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-tech.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "supportsRtl": true
    },
    "plugins": ["expo-router"],
    "platforms": ["ios", "android"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "supportsRTL": true
    },
    "localization": {
      "supportsRTL": true
    },
    "owner": "dennisvaga"
  }
}
